{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "Паста dz-helper by l1nky",
    "description": "я хз что писать",
    "version": "2.01",
    "browser_action": {
        "default_icon": "images/48x48.png",
        "default_title": "Для работы с расширением зайдите на yaklass.ru"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.yaklass.ru/*"
            ],
            "js": [
                "scripts/jquery.js",
                "scripts/yaklass.js"
            ],
            "css": [
                "css/yaklass.css"
            ]
        }
    ],
    "icons": {
        "128": "images/128x128.png",
        "16": "images/16x16.png",
        "48": "images/48x48.png"
    },
    "permissions": [
        "https://www.yaklass.ru/*"
    ]
}